{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Higher education funds spend on Animal Production",
  "width": 800,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/Joshua-Riantoputra/fit3179_hw10/refs/heads/main/STE_2021_AUST_GDA2020.topojson",
    "format": {"type": "topojson", "feature": "aus_state_map"}
  },
  "transform": [
    {
      "lookup": "properties.STE_CODE21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Joshua-Riantoputra/fit3179_hw10/refs/heads/main/data/state_v_socio_HE.csv"
        },
        "key": "code",
        "fields": ["Animal Production and Animal Primary Products"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "title": "Legend",
      "field": "Animal Production and Animal Primary Products",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.STE_NAME21", "type": "nominal", "title": "Country"},
      {"field": "Animal Production and Animal Primary Products", "type": "quantitative"}
    ]
  }
}